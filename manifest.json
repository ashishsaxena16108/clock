{
    "manifest_version" : 2,
    "name" : "Clock",
    "version" : "1.0",
    "description" : "Clock,Timer,Stopwatch",
    "icons" : {
        "128" : "clock128.png",
        "64" : "clock64.png",
        "16" : "clock16.png"
    },
    "browser_action" : {
        "default_icon" : "clock16.png",
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}